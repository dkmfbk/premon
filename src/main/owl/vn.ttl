                                     owl:onProperty pmo:item ;
                                     owl:onClass pmovn:VerbSynItem ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty pmo:example ;
                                     owl:someValuesFrom pmovn:Example
                                   ] .



