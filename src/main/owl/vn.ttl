###  http://premon.fbk.eu/ontology/vn#VerbNetFrame

pmovn:VerbNetFrame rdf:type owl:Class ;
                   
                   rdfs:label "VerbNet syntactic frame"@en ;
                   
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty pmo:item ;
                                     owl:allValuesFrom [ rdf:type owl:Class ;
                                                         owl:unionOf ( pmovn:Pred
                                                                       pmovn:SynItem
                                                                     )
