###  http://premon.fbk.eu/ontology/vn#PrepSynItem

pmovn:PrepSynItem rdf:type owl:Class ;
                  
                  rdfs:subClassOf pmovn:SynItem ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty pmovn:itemValue ;
                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                        owl:intersectionOf ( ontolex:LexicalEntry
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty lexinfo:partOfSpeech ;
                                                                               owl:hasValue lexinfo:preposition
                                                                             ]
                                                                           )
                                                      ]
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty pmovn:restriction ;
                                    owl:allValuesFrom pmovn:PrepositionSelectionalRestriction
                                  ] ,
                                  [ rdf:type owl:Class ;
                                    owl:unionOf ( [ rdf:type owl:Class ;
                                                    owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                           owl:complementOf [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty pmovn:itemValue ;
                                                                                              owl:someValuesFrom owl:Thing
                                                                                            ]
                                                                         ]
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty pmovn:restriction ;
                                                                           owl:someValuesFrom owl:Thing
                                                                         ]
                                                                       )
                                                  ]
                                                  [ rdf:type owl:Class ;
                                                    owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                           owl:complementOf [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty pmovn:restriction ;
                                                                                              owl:someValuesFrom owl:Thing
                                                                                            ]
                                                                         ]
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty pmovn:itemValue ;
                                                                           owl:someValuesFrom owl:Thing
                                                                         ]
                                                                       )
                                                  ]
                                                )
                                  ] ;
                  
                  skos:closeMatch <http://www.lexinfo.net/lexinfo#Preposition> .



###  http://premon.fbk.eu/ontology/vn#PrepositionRestrictionProperty

pmovn:PrepositionRestrictionProperty rdf:type owl:Class ;
                                     
                                     owl:equivalentClass [ rdf:type owl:Class ;
                                                           owl:oneOf ( pmovn:dest_prp
                                                                       pmovn:path_prp
                                                                       pmovn:dir_prp
                                                                       pmovn:loc_prp
                                                                       pmovn:spatial_prp
                                                                       pmovn:plural_prp
                                                                       pmovn:src_prp
                                                                       pmovn:dest_dir_prp
                                                                       pmovn:dest_conf_prp
                                                                     )
                                                         ] ;
                                     
                                     rdfs:subClassOf pmovn:SelectionalRestrictionProperty .



###  http://premon.fbk.eu/ontology/vn#PrepositionSelectionalRestriction

pmovn:PrepositionSelectionalRestriction rdf:type owl:Class ;
                                        
                                        rdfs:subClassOf pmovn:SelectionalRestriction ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty pmovn:itemValue ;
                                                          owl:allValuesFrom pmovn:PrepositionRestrictionProperty
                                                        ] .



###  http://premon.fbk.eu/ontology/vn#Restriction

pmovn:Restriction rdf:type owl:Class ;
                  
                  owl:disjointUnionOf ( pmovn:SelectionalRestriction
                                        pmovn:SyntacticRestriction
                                      ) ,
                                      ( pmovn:AtomicRestriction
                                        pmovn:CompoundRestriction
                                      ) .



###  http://premon.fbk.eu/ontology/vn#RoleRestrictionProperty

pmovn:RoleRestrictionProperty rdf:type owl:Class ;
                              
                              owl:equivalentClass [ rdf:type owl:Class ;
                                                    owl:oneOf ( pmovn:machine_rrp
                                                                pmovn:pointy_rrp
                                                                pmovn:animate_rrp
                                                                pmovn:comestible_rrp
                                                                pmovn:scalar_rrp
                                                                pmovn:shape_rrp
                                                                pmovn:region_rrp
                                                                pmovn:communication_rrp
