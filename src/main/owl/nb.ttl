#################################################################
#
#    Object Properties
#
#################################################################


###  http://premon.fbk.eu/ontology/nb#argument

:argument rdf:type owl:FunctionalProperty ,
                   owl:ObjectProperty ;
          
          rdfs:range :Argument ;
          
          rdfs:domain :SemanticRole .



###  http://premon.fbk.eu/ontology/nb#tag

:tag rdf:type owl:ObjectProperty ;
     
     rdfs:label "tag" ;
     
     rdfs:range :Tag .





#################################################################
#
#    Classes
#
#################################################################


###  http://premon.fbk.eu/ontology/nb#Argument

:Argument rdf:type owl:Class ;
          
          rdfs:label "NomBank semantic role" ;
          
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( :Modifier
                                              :NumberedArgument
                                            )
                              ] ;
          
          rdfs:subClassOf skos:Concept .



###  http://premon.fbk.eu/ontology/nb#Modifier

:Modifier rdf:type owl:Class ;
          
          rdfs:label "NomBank modifier role" ;
          
          rdfs:subClassOf :Argument ,
                          [ rdf:type owl:Class ;
                            owl:oneOf ( :argm-cau
                                        :argm-tmp
                                        :argm-dis
                                        :argm-dir
                                        :argm-prd
                                        :argm-ext
                                        :argm-mnr
                                        :argm-loc
                                        :argm-neg
                                        :argm-pnc
                                        :argm-adv
                                      )
                          ] ;
          
          owl:disjointWith :NumberedArgument .



###  http://premon.fbk.eu/ontology/nb#NumberedArgument

:NumberedArgument rdf:type owl:Class ;
                  
                  rdfs:label "NomBank numbered role" ;
                  
                  rdfs:subClassOf :Argument ,
                                  [ rdf:type owl:Class ;
                                    owl:oneOf ( :arg5
                                                :arg4
                                                :arg3
                                                :arg2
                                                :arg1
                                                :arg0
                                              )
                                  ] .



###  http://premon.fbk.eu/ontology/nb#Roleset

:Roleset rdf:type owl:Class ;
         
         rdfs:label "NomBank predicate" ;
         
         rdfs:subClassOf pmo:SemanticClass ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty pmo:semRole ;
                           owl:allValuesFrom :SemanticRole
                         ] .



###  http://premon.fbk.eu/ontology/nb#SemanticRole

:SemanticRole rdf:type owl:Class ;
              
              rdfs:label "NomBank semantic argument" ;
              
              rdfs:subClassOf pmo:SemanticRole ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty [ owl:inverseOf pmo:semRole
                                               ] ;
                                owl:allValuesFrom :Roleset
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :argument ;
                                owl:someValuesFrom :Argument
                              ] .



###  http://premon.fbk.eu/ontology/nb#Tag

:Tag rdf:type owl:Class ;
     
     rdfs:label "tag" .





