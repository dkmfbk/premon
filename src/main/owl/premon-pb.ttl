#################################################################
#
#    Classes
#
#################################################################


###  http://premon.fbk.eu/ns/premon-pb#Aspect

:Aspect rdf:type owl:Class ;
        
        rdfs:label "aspect" ;
        
        rdfs:subClassOf [ rdf:type owl:Class ;
                          owl:oneOf ( :perfect
                                      :progressive
                                    )
                        ] .



###  http://premon.fbk.eu/ns/premon-pb#Example

:Example rdf:type owl:Class ;
         
         rdfs:label "PropBank example" ;
         
         rdfs:subClassOf premon:Example ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :inflection ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty [ owl:inverseOf nif:referenceContext
                                          ] ;
                           owl:allValuesFrom :Markable
                         ] .



###  http://premon.fbk.eu/ns/premon-pb#Form

:Form rdf:type owl:Class ;
      
      rdfs:label "form" ;
      
      rdfs:subClassOf [ rdf:type owl:Class ;
                        owl:oneOf ( :participle
                                    :infinitive
                                    :gerund
                                    :full
                                  )
                      ] .



###  http://premon.fbk.eu/ns/premon-pb#FunctionTag

:FunctionTag rdf:type owl:Class ;
             
             rdfs:label "PropBank function tag" .



###  http://premon.fbk.eu/ns/premon-pb#Inflection

:Inflection rdf:type owl:Class ;
            
            rdfs:label "inflection" .



###  http://premon.fbk.eu/ns/premon-pb#Markable

:Markable rdf:type owl:Class ;
          
          rdfs:subClassOf premon:Markable ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty nif:annotation ;
                            owl:someValuesFrom [ rdf:type owl:Class ;
                                                 owl:unionOf ( :Predicate
                                                               :SemanticArgument
                                                             )
                                               ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty nif:referenceContext ;
                            owl:allValuesFrom :Example
                          ] .



###  http://premon.fbk.eu/ns/premon-pb#ModifierRole

:ModifierRole rdf:type owl:Class ;
              
              rdfs:label "PropBank modifier role" ;
              
              rdfs:subClassOf :FunctionTag ,
                              :SemanticRole ,
                              [ rdf:type owl:Class ;
                                owl:oneOf ( :argm-rcl
                                            :argm-loc
                                            :argm-ext
                                            :argm-dsp
                                            :argm-com
                                            :argm-gol
                                            :argm-prd
                                            :argm-mod
                                            :argm-pnc
                                            :argm-adj
                                            :argm-slc
                                            :argm-dis
                                            :argm-dir
                                            :argm-rec
                                            :argm-neg
                                            :argm-prp
                                            :argm-mnr
                                            :argm-str
                                            :argm-adv
                                            :argm-tmp
                                            :argm-lvb
                                            :argm-cau
                                          )
                              ] ;
              
              owl:disjointWith :NumberedRole ,
                               :Preposition .



###  http://premon.fbk.eu/ns/premon-pb#NumberedRole

:NumberedRole rdf:type owl:Class ;
              
              rdfs:label "PropBank numbered role" ;
              
              rdfs:subClassOf :SemanticRole ,
                              [ rdf:type owl:Class ;
                                owl:oneOf ( :arg5
                                            :arg3
                                            :arg4
                                            :arg1
                                            :arg2
                                            :arg0
                                          )
                              ] .



###  http://premon.fbk.eu/ns/premon-pb#Person

:Person rdf:type owl:Class ;
        
        rdfs:label "person" ;
        
        rdfs:subClassOf [ rdf:type owl:Class ;
                          owl:oneOf ( :third
                                      :other
                                    )
                        ] .



###  http://premon.fbk.eu/ns/premon-pb#Predicate

:Predicate rdf:type owl:Class ;
           
           rdfs:label "PropBank predicate" ;
           
           rdfs:subClassOf premon:Predicate ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty premon:semArg ;
                             owl:allValuesFrom :SemanticArgument
                           ] .



###  http://premon.fbk.eu/ns/premon-pb#Preposition

:Preposition rdf:type owl:Class ;
             
             rdfs:label "preposition" ;
             
             rdfs:subClassOf :FunctionTag .



###  http://premon.fbk.eu/ns/premon-pb#SemanticArgument

:SemanticArgument rdf:type owl:Class ;
                  
                  rdfs:label "PropBank semantic argument" ;
                  
                  rdfs:subClassOf premon:SemanticArgument ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty [ owl:inverseOf premon:semArg
                                                   ] ;
                                    owl:allValuesFrom :Predicate
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty premon:role ;
                                    owl:cardinality "1"^^xsd:nonNegativeInteger
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty premon:role ;
                                    owl:allValuesFrom :SemanticRole
                                  ] .



###  http://premon.fbk.eu/ns/premon-pb#SemanticRole

:SemanticRole rdf:type owl:Class ;
              
              rdfs:label "PropBank semantic role" ;
              
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:unionOf ( :ModifierRole
                                                  :NumberedRole
                                                )
                                  ] ;
              
              rdfs:subClassOf premon:SemanticRole ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty [ owl:inverseOf premon:role
                                               ] ;
                                owl:allValuesFrom :SemanticArgument
                              ] .



###  http://premon.fbk.eu/ns/premon-pb#Tense

:Tense rdf:type owl:Class ;
       
       rdfs:label "tense" ;
       
       rdfs:subClassOf [ rdf:type owl:Class ;
                         owl:oneOf ( :future
                                     :past
                                     :present
                                   )
                       ] .



###  http://premon.fbk.eu/ns/premon-pb#Voice

:Voice rdf:type owl:Class ;
       
       rdfs:label "voice" ;
       
       rdfs:subClassOf [ rdf:type owl:Class ;
                         owl:oneOf ( :active
                                     :passive
                                   )
                       ] .





