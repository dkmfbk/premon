#################################################################
#
#    Object Properties
#
#################################################################


###  http://premon.fbk.eu/ontology/pb#argument

:argument rdf:type owl:FunctionalProperty ,
                   owl:ObjectProperty ;
          
          rdfs:range :Argument ;
          
          rdfs:domain :SemanticRole .



###  http://premon.fbk.eu/ontology/pb#aspect

:aspect rdf:type owl:ObjectProperty ;
        
        rdfs:label "aspect" ;
        
        rdfs:range :Aspect ;
        
        rdfs:domain :Inflection .



###  http://premon.fbk.eu/ontology/pb#form

:form rdf:type owl:FunctionalProperty ,
               owl:ObjectProperty ;
      
      rdfs:label "form" ;
      
      rdfs:range :Form ;
      
      rdfs:domain :Inflection .



###  http://premon.fbk.eu/ontology/pb#inflection

:inflection rdf:type owl:FunctionalProperty ,
                     owl:ObjectProperty ;
            
            rdfs:label "inflection" ;
            
            rdfs:domain pmo:Example ;
            
            rdfs:range :Inflection .



###  http://premon.fbk.eu/ontology/pb#person

:person rdf:type owl:FunctionalProperty ,
                 owl:ObjectProperty ;
        
        rdfs:label "person" ;
        
        rdfs:domain :Inflection ;
        
        rdfs:range :Person .



###  http://premon.fbk.eu/ontology/pb#tag

:tag rdf:type owl:FunctionalProperty ,
              owl:ObjectProperty ;
     
     rdfs:label "function tag" ;
     
     rdfs:range [ rdf:type owl:Class ;
                  owl:unionOf ( :Modifier
                                :Tag
                                ontolex:LexicalEntry
                              )
                ] .



###  http://premon.fbk.eu/ontology/pb#tense

:tense rdf:type owl:FunctionalProperty ,
                owl:ObjectProperty ;
       
       rdfs:label "tense" ;
       
       rdfs:domain :Inflection ;
       
       rdfs:range :Tense .



###  http://premon.fbk.eu/ontology/pb#voice

:voice rdf:type owl:FunctionalProperty ,
                owl:ObjectProperty ;
       
       rdfs:label "voice" ;
       
       rdfs:domain :Inflection ;
       
       rdfs:range :Voice .





#################################################################
#
#    Classes
#
#################################################################


###  http://premon.fbk.eu/ontology/pb#Argument

:Argument rdf:type owl:Class ;
          
          rdfs:label "PropBank semantic role" ;
          
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( :Modifier
                                              :NumberedArgument
                                              :SecondaryAgent
                                            )
                              ] ;
          
          rdfs:subClassOf ontolex:LexicalConcept .



###  http://premon.fbk.eu/ontology/pb#Aspect

:Aspect rdf:type owl:Class ;
        
        rdfs:label "aspect" ;
        
        rdfs:subClassOf [ rdf:type owl:Class ;
                          owl:oneOf ( :perfect
                                      :progressive
                                    )
                        ] .



###  http://premon.fbk.eu/ontology/pb#Form

:Form rdf:type owl:Class ;
      
      rdfs:label "form" ;
      
      rdfs:subClassOf [ rdf:type owl:Class ;
                        owl:oneOf ( :gerund
                                    :full
                                    :participle
                                    :infinitive
                                  )
                      ] .



###  http://premon.fbk.eu/ontology/pb#Inflection

:Inflection rdf:type owl:Class ;
            
            rdfs:label "inflection" .



###  http://premon.fbk.eu/ontology/pb#Modifier

:Modifier rdf:type owl:Class ;
          
          rdfs:label "PropBank modifier role" ;
          
          rdfs:subClassOf :Argument ,
                          [ rdf:type owl:Class ;
                            owl:oneOf ( :argm-lvb
                                        :argm-cau
                                        :argm-tmp
                                        :argm-rcl
                                        :argm-loc
                                        :argm-gol
                                        :argm-mod
                                        :argm-prd
                                        :argm-com
                                        :argm-dsp
                                        :argm-ext
                                        :argm-adj
                                        :argm-pnc
                                        :argm-slc
                                        :argm-neg
                                        :argm-rec
                                        :argm-dis
                                        :argm-prp
                                        :argm-dir
                                        :argm-adv
                                        :argm-mnr
                                      )
                          ] ;
          
          owl:disjointWith :NumberedArgument ,
                           :SecondaryAgent .



###  http://premon.fbk.eu/ontology/pb#NumberedArgument

:NumberedArgument rdf:type owl:Class ;
                  
                  rdfs:label "PropBank numbered role" ;
                  
                  rdfs:subClassOf :Argument ,
                                  [ rdf:type owl:Class ;
                                    owl:oneOf ( :arg4
                                                :arg3
                                                :arg2
                                                :arg1
                                                :arg0
                                                :arg5
                                              )
                                  ] ;
                  
                  owl:disjointWith :SecondaryAgent .



###  http://premon.fbk.eu/ontology/pb#Person

:Person rdf:type owl:Class ;
        
        rdfs:label "person" ;
        
        rdfs:subClassOf [ rdf:type owl:Class ;
                          owl:oneOf ( :other
                                      :third
                                    )
                        ] .



###  http://premon.fbk.eu/ontology/pb#Roleset

:Roleset rdf:type owl:Class ;
         
         rdfs:label "PropBank predicate" ;
         
         rdfs:subClassOf pmo:SemanticClass ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty pmo:semRole ;
                           owl:allValuesFrom :SemanticRole
                         ] .



###  http://premon.fbk.eu/ontology/pb#SecondaryAgent

:SecondaryAgent rdf:type owl:Class ;
                
                rdfs:label "PropBank agentive role" ;
                
                rdfs:subClassOf :Argument .



###  http://premon.fbk.eu/ontology/pb#SemanticRole

:SemanticRole rdf:type owl:Class ;
              
              rdfs:label "PropBank semantic argument" ;
              
              rdfs:subClassOf pmo:SemanticRole ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :argument ;
                                owl:someValuesFrom :Argument
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty [ owl:inverseOf pmo:semRole
                                               ] ;
                                owl:allValuesFrom :Roleset
                              ] .



###  http://premon.fbk.eu/ontology/pb#Tag

:Tag rdf:type owl:Class ;
     
     rdfs:label "PropBank function tag" .



###  http://premon.fbk.eu/ontology/pb#Tense

:Tense rdf:type owl:Class ;
       
       rdfs:label "tense" ;
       
       rdfs:subClassOf [ rdf:type owl:Class ;
                         owl:oneOf ( :future
                                     :past
                                     :present
                                   )
                       ] .



###  http://premon.fbk.eu/ontology/pb#Voice

:Voice rdf:type owl:Class ;
       
       rdfs:label "voice" ;
       
       rdfs:subClassOf [ rdf:type owl:Class ;
                         owl:oneOf ( :passive
                                     :active
                                   )
                       ] .





